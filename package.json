{
  "name": "madig",
  "version": "0.2.0",
  "description": "Javascript dependency injection engine",
  "main": "index.js",
  "scripts": {
    "test": "nyc -r lcov -r text-summary mocha",
    "lint": "eslint src/*.js"
  },
  "nyc": {
    "report-dir": "./build/coverage",
    "check-coverage": true,
    "statements": 90,
    "exclude": [
      "**/*.spec.js", "src/resources/**"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write --parser json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drouian-m/madig.git"
  },
  "keywords": ["ioc", "dependency-injection", "ioc-container", "injection-container"],
  "author": "Matthieu Drouian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drouian-m/madig/issues"
  },
  "homepage": "https://github.com/drouian-m/madig#readme",
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "mocha": "^8.3.2",
    "mocha-multi-reporters": "^1.5.1",
    "mocha-suppress-logs": "^0.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "sinon": "^10.0.0"
  },
  "dependencies": {
    "debug": "^4.3.1",
    "joi": "^17.4.0"
  }
}
